   "source": [
    "df6 = df5[~(df5.total_sqft/df5.BHK<300)] # ~( ) this is used to negate or remove these outliers or data\n",
    "df6.shape # gives rows and columns"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# since 1 bed shall atleast have 300 sqft so we removed those outliers."
